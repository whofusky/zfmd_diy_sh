# ~/.profile: executed by the command interpreter for login shells.
# This file is not read by bash(1), if ~/.bash_profile or ~/.bash_login
# exists.
# see /usr/share/doc/bash/examples/startup-files for examples.
# the files are located in the bash-doc package.

# the default umask is set in /etc/profile; for setting the umask
# for ssh logins, install and configure the libpam-umask package.
#umask 022

# if running bash
if [ -n "$BASH_VERSION" ]; then
    # include .bashrc if it exists
    if [ -f "$HOME/.bashrc" ]; then
	. "$HOME/.bashrc"
    fi
#else
#	export http_proxy=http://127.0.0.1:7890
#	export https_proxy=http://127.0.0.1:7890
fi

# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/bin" ] ; then
    PATH="$HOME/bin:$PATH"
fi

# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/.local/bin" ] ; then
    PATH="$HOME/.local/bin:$PATH"
fi
if [ -d "$HOME/mysoft/aliyunpan-v0.2.1-linux-amd64" ] ; then
    PATH="$HOME/mysoft/aliyunpan-v0.2.1-linux-amd64:$PATH"
fi

if [ -d "$HOME/lib" ]; then
    export LD_LIBRARY_PATH=$HOME/lib
fi

aa=1234
bb=456
[daemon]
export aa='12345'
export bb='12345'
[daemon]
kk='find . -name ".*.swp" -type f -print|while read tnaa;do echo "rm -rf ${tnaa}";rm -rf "${tnaa}";done'
alias kk='find . -name ".*.swp" -type f -print|while read tnaa;do echo "rm -rf ${tnaa}";rm -rf "${tnaa}";done'
export cc='12345'
alias cddn='cd /home/fusky/Downloads'
alias rmvimswp='find . -name ".*.swp" -type f -print|while read tnaa;do echo "rm -rf ${tnaa}";rm -rf "${tnaa}";done'
