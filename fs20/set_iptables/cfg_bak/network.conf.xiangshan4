#database_port=1521      #数据库访问端口

# |${prtAll}/${srvop_null}/${op_all}//${scada_local_intranet_ip}/${AUDIT_IP}

cfg_rule_items_scada="
   ${prtAll}/${srvOp_client}/${op_tcp}//${scada_local_intranet_ip}/${ps1_local_intranet_ip}:${DC_PORT}
  |${prtAll}/${srvop_null}/${op_tcp}//${scada_local_intranet_ip}:${TIME_PORT}/${ps1_local_intranet_ip},${wk_local_intranet_ip}:${TIME_PORT}
  |${prtAll}/${srvOp_server}/${op_tcp}//${scada_local_cftzf_ip}:2404/100.100.100.99,100.100.100.235,100.100.100.236
  |${prtAll}/${srvOp_client}/${op_tcp}//${scada_local_cft_ip}/192.168.1.200:502
  |${prtAll}/${srvOp_client}/${op_tcp}//${scada_local_fj_ip}/192.168.100.7,192.168.100.8,192.168.100.9:502
"

cfg_rule_ssh_scada="
     ${prtAll}/${srvOp_server}/${op_tcp}//${scada_local_intranet_ip}:${ssh_port}/${ps1_local_intranet_ip},${wk_local_intranet_ip}:${ssh_ctPort}
     |${prtAll}/${srvOp_client}/${op_tcp}//${scada_local_intranet_ip}:${ssh_ctPort}/${ps1_local_intranet_ip},${wk_local_intranet_ip}:${ssh_port}
"



