  | ${prtAll}/${srvop_null}/${op_tcp}//${ps1_local_intranet_ip}:${TIME_PORT}/${scada_local_intranet_ip},${wk_local_intranet_ip}:${TIME_PORT}
  | ${prtAll}/${srvOp_server}/${op_tcp}//${ps1_upload_localip}:3000/${upload_remote_ips}
  | ${prtAll}/${srvOp_client}/${op_tcp}//175.1.5.104/175.1.1.1,175.1.1.2:22
"

cfg_rule_ssh_ps1="
     ${prtAll}/${srvOp_server}/${op_tcp}//${ps1_local_intranet_ip}:${ssh_port}/${scada_local_intranet_ip},${wk_local_intranet_ip}:${ssh_ctPort}
     |${prtAll}/${srvOp_client}/${op_tcp}//${ps1_local_intranet_ip}:${ssh_ctPort}/${scada_local_intranet_ip},${wk_local_intranet_ip}:${ssh_port}
"

